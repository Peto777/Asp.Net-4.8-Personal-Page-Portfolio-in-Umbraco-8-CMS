@model PeterGlozikUmbracoOsobnaStranka.lib.Models.DashboardBlog.CKEditorModel

<script src="@Url.Content("~/Styles/ckeditor/ckeditor.js")" type="text/javascript"></script>
<script>
    $(document).ready(function () {
        $(".@Model.CKEditorClass").each(function () {
            CKEDITOR.replace($(this).attr('id'), {
                height: @Model.CKEditorHeight
            });
        });
    });
</script>
